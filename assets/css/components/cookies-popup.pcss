.cookies-popup {
    @apply fixed w-full bottom-0 py-6 flex justify-center;
    &__inner-wrapper {
        @apply flex items-center gap-8 text-black uppercase text-xs font-medium bg-pink py-2 px-6 rounded-full;

        a {
            @apply text-white;
        }

        img {
            @apply h-5 cursor-pointer;
        }
    }
}